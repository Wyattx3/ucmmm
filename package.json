{
  "name": "ucera-frontend",
  "version": "1.0.0",
  "description": "UC ERA Frontend Application - 11-step registration system with Appwrite backend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview:prod": "vite preview --port 3000",
    "test": "echo \"No tests configured yet\" && exit 0",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postbuild": "echo \"âœ… Production build completed successfully!\"",
    "setup:env": "node setup-vercel-env.cjs",
    "setup:database": "node setup-database.cjs",
    "setup:complete": "npm run setup:env && npm run setup:database",
    "setup:all": "node run-complete-setup.cjs",
    "test:deployment": "node test-deployment.cjs",
    "deploy": "vercel --prod",
    "deploy:force": "vercel --prod --force",
    "logs": "vercel logs",
    "env:list": "vercel env ls",
    "env:pull": "vercel env pull .env.local"
  },
  "keywords": [
    "ucera",
    "registration",
    "frontend",
    "react",
    "vite",
    "appwrite",
    "authentication",
    "myanmar"
  ],
  "author": "UC ERA Development Team",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "appwrite": "^20.1.0",
    "dotenv": "^17.2.1",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.21",
    "html2canvas": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "archiver": "^7.0.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^16.3.0",
    "node-appwrite": "^19.1.0",
    "node-fetch": "^3.3.2",
    "terser": "^5.43.1",
    "vite": "^5.0.8"
  }
}
